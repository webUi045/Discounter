./../store/reducers/discounterReducer\";\nimport loader from \"../../assets/images/loader.gif\";\nimport { IShop } from \"../../types\";\nimport \"./MainPage.scss\";\n\ninterface IState {\n  store: {\n    shops: IShop[];\n    loading: boolean;\n  };\n}\n\nconst MainPage = () => {\n  const dispatch = useDispatch();\n  const shops = useSelector((state: IState) => state.store.shops);\n  const { loading } = useSelector((state: { store: IInitialState }) => state.store);\n\n  useEffect(() => {\n    dispatch(requestShops());\n    dispatch(requestAuthorizationCheck());\n  }, []);\n\n  return (\n    <main className=\"main\">\n      <div className=\"main__wrapper\">\n        {loading ?\n          <div className=\"main__loader-container\">\n            <img className=\"loader\" src={loader} alt=\"loader\" />\n          </div> :\n          <>\n            <h2 className=\"main__title\">News</h2>\n            {shops.map((shop: IShop) => <NewsCard key={shop.id} shop={shop} />)}\n          </>\n        }\n      </div>\n    </main>\n  );\n};\n\nexport default MainPage;\n","/home/irina/SoftServe/discounter/Discounter/src/shared/Layout/Layout.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/pages/ProfilePage/ProfilePage.tsx",["208","209","210","211","212"],"/home/irina/SoftServe/discounter/Discounter/src/firebaseConfig.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Footer/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/NewsCard/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Header/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Button/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Input/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Footer/Footer.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/NewsCard/NewsCard.tsx",["213"],"import { IShop } from \"../../types\";\nimport \"./NewsCard.scss\";\n\ninterface INewsCardProps {\n  shop: IShop;\n}\n\nconst NewsCard = (props: INewsCardProps) => {\n  const { shop } = props;\n  return (\n    <div className=\"news-card\">\n      <div className=\"news-card__wrapper\">\n        <img className=\"news-card__img\" src={shop.img} alt={shop.store} />\n        <h3 className=\"news-card__title\">{shop.title}</h3>\n        <a className=\"news-card__store\">{shop.store}</a>\n        <p className=\"news-card__description\">{shop.description}</p>\n      </div>\n    </div>\n  );\n};\n\nexport default NewsCard;\n","/home/irina/SoftServe/discounter/Discounter/src/shared/Header/Header.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Button/Button.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Input/Input.tsx",["214","215"],"import React, {MutableRefObject, RefObject} from \"react\";\nimport \"./Input.scss\";\ninterface IInputProps {\n  autoFocus?: boolean\n  value?: string,\n  onChange(term: string): void,\n  type: string,\n  placeholder?: string,\n  style: string,\n  disabled?: boolean,\n  onBlur?(): void,\n // ref?(): MutableRefObject<HTMLInputElement | null>,\n  id?: string,\n  refInput?: MutableRefObject<HTMLInputElement | null>,\n}\n\nconst Input = ({ autoFocus, value, onChange, type, placeholder, style, disabled, onBlur, refInput, id }: IInputProps) => {\n\n  return (\n    <input\n      //autoFocus={autoFocus}\n      type={type}\n      name=\"input\"\n      className={style}\n      value={value}\n      onChange={(e) => {\n        onChange(e.target.value)\n      }}\n      placeholder={placeholder}\n      required\n      disabled={disabled}\n      onBlur={onBlur}\n      ref={refInput}\n      id={id}\n    />\n  );\n};\n\nexport default Input;\n","/home/irina/SoftServe/discounter/Discounter/src/shared/Logo/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Navigation/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Navigation/Navigation.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/Logo/Logo.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/PrivateNav/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/PublicNav/index.ts",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/PrivateNav/PrivateNav.tsx",[],"/home/irina/SoftServe/discounter/Discounter/src/shared/PublicNav/PublicNav.tsx",["216","217","218","219","220","221"],"import React, { useState } from \"react\";\nimport Button from \"../Button\";\nimport Input from \"../Input\";\nimport Modal from \"../Modal\";\nimport \"./PublicNav.scss\";\n\ninterface IPublicNavProps {\n  email: string;\n  password: string;\n  firstName: string;\n  lastName: string;\n  onChangeName(name: string): void;\n  onChangeLastName(lastName: string): void;\n  onChangeEmail(email: string): void;\n  onChangePassword(password: string): void;\n  emailError: string;\n  passwordError: string;\n  handleSignUp(): void;\n  handleSignIn(): void;\n  handleInputs(): void;\n}\n\nconst PublicNav = ({\n  email,\n  password,\n  firstName,\n  lastName,\n  onChangeEmail,\n  onChangePassword,\n  emailError,\n  passwordError,\n  handleSignUp,\n  handleSignIn,\n  handleInputs,\n  onChangeLastName,\n  onChangeName,\n}: IPublicNavProps) => {\n  const [isOpenSignIn, setIsOpenSignIn] = useState(false);\n  const [isOpenSignUp, setIsOpenSignUp] = useState(false);\n\n  const onClickHandlerSignIn = () => {\n    setIsOpenSignIn(!isOpenSignIn);\n  };\n\n  const onClickHandlerSignUp = () => {\n    setIsOpenSignUp(!isOpenSignUp);\n  };\n\n  const onClickCloseModalSignUp = () => {\n    setIsOpenSignUp(false);\n    handleInputs();\n  };\n\n  const onClickCloseModalSignIn = () => {\n    setIsOpenSignIn(false);\n    handleInputs();\n  };\n\n  return (\n    <>\n      <Button onClick={onClickHandlerSignUp} className=\"nav__button button\">\n        Sign up\n      </Button>\n      {isOpenSignUp && (\n        <Modal onClick={onClickCloseModalSignUp} name=\"Sign up\">\n          <div className=\"signup-inputs\">\n            <div